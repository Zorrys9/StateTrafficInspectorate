<Window x:Class="StateTrafficInspectorate.ReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StateTrafficInspectorate"
        mc:Ignorable="d"
        Title="Отчет по водителям" Height="227" Width="814">
    <Grid Loaded="Grid_Loaded">
        <DataGrid x:Name="ReportList" HorizontalAlignment="Left" Height="176" Margin="10,10,0,0" VerticalAlignment="Top" Width="635"/>
        <Button x:Name="Save" Content="Сохранить" HorizontalAlignment="Left" Margin="656,10,0,0" VerticalAlignment="Top" Width="137" Height="50" Click="Save_Click"/>
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Left" Margin="656,65,0,0" VerticalAlignment="Top" Width="137" Height="50" Click="Back_Click"/>

    </Grid>
</Window>
