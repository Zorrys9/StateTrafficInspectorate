<Window x:Class="StateTrafficInspectorate.UnRegisteredUser.CheckTransport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StateTrafficInspectorate.UnRegisteredUser"
        mc:Ignorable="d"
        Title="Проверка транспорта" Height="166" Width="318" Style="{StaticResource StyleWindow}">
    <Grid Background="#FFDFF0FA">
        <Label Content="VIN " FontSize="16" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="VINcode" MaxLength="17" HorizontalAlignment="Left" Height="31" Margin="80,10,0,0" TextWrapping="Wrap" FontSize="18" Text="" VerticalAlignment="Top" Width="215"/>
        <Button x:Name="Check" Content="Проверить" FontSize="16" HorizontalAlignment="Left" Margin="160,59,0,0" VerticalAlignment="Top" Width="135" Height="45" Click="Check_Click"/>
        <Button x:Name="Back" Content="Назад" FontSize="16" HorizontalAlignment="Left" Margin="10,59,0,0" VerticalAlignment="Top" Width="135" Height="45" Click="Back_Click"/>

    </Grid>
</Window>
